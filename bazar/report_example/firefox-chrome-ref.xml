<?xml version="1.0" encoding="UTF-8"?>
<tests>
    <test>
        <diagnostic>
            <name>
                pixelscomparison
            </name>
            <reference>
                bmp/ref.bmp
            </reference>
            <parameters/>
        </diagnostic>
        <comparisons>
            <comparison>
                <instances>
                    <instance>
                        <instance_id>
                            1_google-chrome
                        </instance_id>
                        <preprocessing>
                            <output>
                                results/14480/preprocessing/wp-1-1_google-chrome-0-identity.svg
                            </output>
                            <preprocess>
                                <name>
                                    identity
                                </name>
                                <parameters/>
                                <filter_id>
                                    0
                                </filter_id>
                            </preprocess>
                        </preprocessing>
                        <capture>
                            <name>
                                statique-cap
                            </name>
                            <output>
                                results/14480/capture/wp-1-1_google-chrome-statique-cap.bmp
                            </output>
                            <parameters>
                                <browser>
                                    google-chrome
                                </browser>
                            </parameters>
                        </capture>
                        <postprocessing>
                            <output>
                                results/14480/postprocessing/wp-1-1_google-chrome-1-statique.bmp
                            </output>
                            <postprocess>
                                <name>
                                    identity
                                </name>
                                <parameters/>
                                <filter_id>
                                    0
                                </filter_id>
                            </postprocess>
                            <postprocess>
                                <name>
                                    statique
                                </name>
                                <parameters/>
                                <filter_id>
                                    1
                                </filter_id>
                            </postprocess>
                        </postprocessing>
                        <browser>
                            google-chrome
                        </browser>
                    </instance>
                </instances>
                <result>
                    <message>
                        taille des images differente
                    </message>
                    <valid>
                        False
                    </valid>
                </result>
            </comparison>
            <comparison>
                <instances>
                    <instance>
                        <instance_id>
                            2_firefox
                        </instance_id>
                        <preprocessing>
                            <output>
                                results/14480/preprocessing/wp-1-2_firefox-0-identity.svg
                            </output>
                            <preprocess>
                                <name>
                                    identity
                                </name>
                                <parameters/>
                                <filter_id>
                                    0
                                </filter_id>
                            </preprocess>
                        </preprocessing>
                        <capture>
                            <name>
                                statique-cap
                            </name>
                            <output>
                                results/14480/capture/wp-1-2_firefox-statique-cap.bmp
                            </output>
                            <parameters>
                                <browser>
                                    firefox
                                </browser>
                            </parameters>
                        </capture>
                        <postprocessing>
                            <output>
                                results/14480/postprocessing/wp-1-2_firefox-1-statique.bmp
                            </output>
                            <postprocess>
                                <name>
                                    identity
                                </name>
                                <parameters/>
                                <filter_id>
                                    0
                                </filter_id>
                            </postprocess>
                            <postprocess>
                                <name>
                                    statique
                                </name>
                                <parameters/>
                                <filter_id>
                                    1
                                </filter_id>
                            </postprocess>
                        </postprocessing>
                        <browser>
                            firefox
                        </browser>
                    </instance>
                </instances>
                <result>
                    <message>
                        Egalité pixel à pixel.
                    </message>
                    <valid>
                        True
                    </valid>
                </result>
            </comparison>
        </comparisons>
    </test>
</tests>
