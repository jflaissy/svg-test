<?xml version="1.0" encoding="UTF-8"?>
  <!-- '''Fichier de configuration pour les tests SVG''' -->
<tests > 
  <test>
    <source> <!--Fichier svg --></source>
    <type>
      <!--Tous les browsers Séparement(éventuellement un seul), les browsers les  uns contre les autres --> 
    </type>
    <browsers><!--Ici on définit tous les navigateurs dont on a besoin-->
      <browser></browser>
    </browsers>
    <preprocessing>
      <!-- Ici on met tous les preprocess dont on a besoin (filtre sur le svg ,...)-->
      <preprocess>
	<name><!-- Nom du fichier (python ) qui fait le preprocess sans l'extension .py--></name>
	<parameters><!--L'utilisateur peut définir ses propres parameter avec des noms a lui --></parameters>
      </preprocess>
    </preprocessing>

    <capture>
      <name><!--Nom du fichier sans l'extension qui réalise la capture--></name>
      <interactions>
	<!-- Script d'interacion si besoin-->
      </interactions>
      
      <parameters> 
	<!--Parametres eventuels pour la capture-->
	<parameter></parameter>
      </parameters>
    
    </capture>

   <postprocessing>
     <!--Traitement post-capture mais avant le diagnostique final on met tous les post-process dont on a besoin-->
        <postprocess>
            <name><!--Nom du fichier sans l'extension qui permet le preprocess--></name>
            <parameters>
	      <parameter></parameter>
	    </parameters>
        </postprocess>
    </postprocessing>
   
    <diagnostic>
      <name><!--Nom du fichier sans l'extension qui fais le diagnostic--></name>
      <parameters><!--Liste de tous les parametres-->
	<parameter></parameter>
      </parameters>

      <reference><!--Image de référence pour effectuer une comparaison--></reference>
    </diagnostic>

  </test>
  
  <test> 
    <!--Autre test --> 
  </test>
</tests>
